{
    "name": "arno1979/pdf-filename-mod",
    "type": "contao-bundle",
    "description": "Modifies the filename of a PDF generated from an article to match the article's alias",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Arno Schumacher",
            "homepage": "https://github.com/arno1979"
        }
    ],
    "require": {
        "php": "^7.1",
        "contao/core-bundle": "~4.3",
        "symfony/framework-bundle": "^3.4"
    },
    "conflict": {
        "contao/core": "*",
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0"
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "1.0.x-dev"
        },
        "contao-manager-plugin": "Arno1979\\PdfFilenameMod\\ContaoManager\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "Arno1979\\PdfFilenameMod\\": "src/"
        },
        "classmap": [
            "src/Resources/contao/"
        ],
        "exclude-from-classmap": [
            "src/Resources/contao/config/",
            "src/Resources/contao/dca/",
            "src/Resources/contao/languages/",
            "src/Resources/contao/templates/"
        ]
    }
}
